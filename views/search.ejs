<!-- views/index.ejs -->
<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
</head>
<body>
  <h1><%= title %></h1>
   <!-- In views/index.ejs -->
<ul>
    <% if (locals.users) {%>
      <% users.forEach(function(user) { %>
        <li>ID: <%= user.id %>, Name: <%= user.firstName %> <%= user.lastName %>.
          Email: <%= user.email %>, Age: <%= user.age %>, Bio: <%= user.bio %>
        </li>
      <% }); %>
    <% } %>
  </ul>

  
  <a href="/create">Create a user</a>
</body>
</html>